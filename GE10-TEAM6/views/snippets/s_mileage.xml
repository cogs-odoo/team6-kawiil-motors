<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_mileage" name="Countdown">
    <section class="s_mileage pt48 pb48">
        <div class="container">
            <div class="s_mileage_canvas_wrapper text-center"/>
        </div>
    </section>
</template>

<template id="s_mileage_options" inherit_id="website.snippet_options">
    <xpath expr="." position="inside">
        <div data-js="mileage" data-selector=".s_mileage">
            <!-- <we-colorpicker string="Text Color" data-attribute-name="textColor" data-select-data-attribute=""/> -->
        </div>
    </xpath>
</template>

<record id="GE10-TEAM6.s_mileage_000_js" model="ir.asset">
    <field name="name">Mileage 000 JS</field>
    <field name="bundle">web.assets_frontend</field>
    <field name="path">GE10-TEAM6/static/src/snippets/s_mileage/000.js</field>
</record>

</odoo>
